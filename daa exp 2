#include<bits/stdc++.h> 
using namespace std; 
double power(double n,int x)
{ if(x==0)
return 1;
double temp=power(n,x/2); if(x%2==0) {
return temp*temp; }
else if(x>0) {
    return n*temp*temp;
} else
return (temp*temp)/n;
} int main()
{ double n;
int x; cin>>n>>x;
double ans=power(n,x); cout<<"Output: "<<ans<<endl; }
